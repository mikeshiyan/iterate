{
    "name": "shiyan/iterate",
    "description": "Iterates iterators by a scenario.",
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Shiyan\\Iterate\\": "src/"
        }
    },
    "keywords": ["iterator", "scenario", "regex", "match", "php", "oop"],
    "support": {
        "source": "https://github.com/mikeshiyan/iterate",
        "issues": "https://github.com/mikeshiyan/iterate/issues"
    },
    "authors": [
        {
            "name": "Mike Shiyan",
            "homepage": "https://github.com/mikeshiyan"
        }
    ],
    "require-dev": {
        "symfony/console": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Shiyan\\Iterate\\tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text --whitelist src tests"
    }
}
