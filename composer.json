    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Shiyan\\Iterate\\": "src/"
        }
    },
    "keywords": ["regex", "match", "iterator", "scenario", "php", "oop"],
    "support": {
        "source": "https://github.com/mikeshiyan/iterator-regex",
        "issues": "https://github.com/mikeshiyan/iterator-regex/issues"
    },
    "authors": [
        {
            "name": "Mike Shiyan",
            "homepage": "https://github.com/mikeshiyan"
        }
